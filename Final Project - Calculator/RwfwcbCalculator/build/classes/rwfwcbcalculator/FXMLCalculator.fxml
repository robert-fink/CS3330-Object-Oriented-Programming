            <Button mnemonicParsing="false" onAction="#inputOperator" prefHeight="50.0" prefWidth="125.0" text="-" />
         </children>
      </HBox>
      <HBox layoutX="18.0" layoutY="199.0" prefHeight="37.0" prefWidth="467.0">
         <children>
            <Button mnemonicParsing="false" onAction="#inputNumpad" prefHeight="37.0" prefWidth="125.0" text="7" />
