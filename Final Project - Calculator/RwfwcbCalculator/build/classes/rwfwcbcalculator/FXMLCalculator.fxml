         </children>
      </HBox>
      <HBox layoutX="19.0" layoutY="127.0" prefHeight="37.0" prefWidth="467.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleSave" prefHeight="35.0" prefWidth="235.0" text="Save Answer" />
            <Button mnemonicParsing="false" onAction="#handleOpen" prefHeight="35.0" prefWidth="235.0" text="Recall Answer" />
         </children>
      </HBox>
   </children>
